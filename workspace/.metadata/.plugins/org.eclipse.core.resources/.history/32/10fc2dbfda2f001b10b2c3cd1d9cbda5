
public class Testing {
	public static void main(String[] args) {
		int n = 14;
		int m = 21;
		int[] answer = new int[2];
        if(n>m){
            int temp = n;
            n = m;
            m = temp;
        }
        
        for(int i=1; i<=n; i++){
            if(n%answer[0]==0 && m%answer[0]==0)
                answer[0] = i;
        }
                
        answer[1] = answer[0] *n * m;
       
        System.out.println(n+" "+m);
        
    }
	}
	



