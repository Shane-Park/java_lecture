package b_operator;

public class ComparingOperator {

	public static void main(String[] args) {
		/*
		 * 비교연산자
		 * - <,>,<=,>=,==,!=
		 * - 문자열 비교 : equals()
		 * 
		 */

		int x = 10;
		int y = 20;
		boolean b = x < y; // 비교 연산자의 연산 결과는 boolean 이다.
		System.out.println(b);
		
		b = x <= y - 15; // 산술 연산 후 비교 연산을 수행 한다.
		System.out.println(b);
		
		String str1 = "abc";
		String str2 = "abc";
		b = str1 == str2; // 문자열의 내용이 아닌 주소를 비교 한 것이다.
		System.out.println(b);
		
	}

}
