package programmers;

public class GetMiddleNumber {

	static String solution(String s) {
		String answer = "";
		if(s.length()%2==0)
			answer += s.substring(s.length()/2);
			return answer;
	}

	public static void main(String[] args) {
		String s = "abcde";
		System.out.println(solution(s));

	}

}
