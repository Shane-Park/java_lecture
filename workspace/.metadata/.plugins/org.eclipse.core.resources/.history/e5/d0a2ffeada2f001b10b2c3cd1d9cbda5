
public class Testing {
	public static void main(String[] args) {
		int n = 3;
		int m = 12;
		int[] answer = new int[2];
		answer[0] = 1;
        if(n>m){
            int temp = n;
            n = m;
            m = temp;
        }
        
        for(int i=2; i<=n; i++){
            if(n%answer[0]==0 && m%answer[0]==0)
                answer[0] = i;
        }
                
        answer[1] = answer[0] *n * m;
       
        System.out.println(n+" "+m+answer[0]+" "+answer[1]);
        
    }
	}
	



